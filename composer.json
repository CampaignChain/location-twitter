{
    "name": "campaignchain/location-twitter",
    "description": "Integrates Twitter locations such as user stream and status.",
    "keywords": ["twitter","campaignchain"],
    "type": "campaignchain-location",
    "homepage": "http://www.campaignchain.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "CampaignChain Inc.",
            "email": "info@campaignchain.com",
            "homepage": "http://www.campaignchain.com"
        }
    ],
    "support": {
        "issues": "https://github.com/CampaignChain/campaignchain-ce/issues",
        "docs": "http://doc.campaignchain.com"
    },
    "require": {
        "campaignchain/channel-twitter": "1.0.0-beta.2"
    },
    "autoload": {
        "psr-4": { "CampaignChain\\Location\\TwitterBundle\\": "" }
    },
    "extra": {
        "campaignchain": {
            "kernel": {
